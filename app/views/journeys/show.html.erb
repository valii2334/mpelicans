<h1 class="h3 mb-3">
  <%= @journey.title %>
</h1>

<div class="row">
	<div class="col-12">
		<div class="card">
			<div class="card-body">
        <iframe
          width="100%"
          height="450"
          style="border:0"
          loading="lazy"
          allowfullscreen
          referrerpolicy="no-referrer-when-downgrade"
          src="<%= @journey.map_url %>">
				</iframe>
      </div>
    </div>
  </div>
</div>

<div class="row">
	<div class="col-12">
		<div class="card">
      <div class="card-header">
        <h4>
          First picture.
        </h4>
      </div>
			<div class="card-body">
        <%= image_tag @journey.image, class: 'd-block w-100' %>
      </div>
    </div>
  </div>
</div>
